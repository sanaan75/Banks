@page
@model Banks.Pages._App.Conferences.ImportConferenceFromExcel

<div class="row mt-5">
    <div class="col-12 px-3">
        <div class="card">
            <div class="card-body">
                <form enctype="multipart/form-data" method="post">
                    <div class="col-12 text-center py-3">
                        <h3 class="text-center"> خوانش از اکسل </h3>
                    </div>
                    
                    <div class="form-group mb-3">
                        <label asp-for="@Model.FileModel.FormFile" class="text-right"></label>
                        <input type="text" asp-for="@Model.FileModel.FormFile"type="file">
                        <span class="text-danger" asp-validation-for="@Model.FileModel.FormFile"></span>
                    </div>

                    <div class="col-12">
                        <button class="btn btn-primary px-5 text-center" type="submit">خوانش</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>